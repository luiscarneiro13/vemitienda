{
  "expo": {
    "name": "Ve mi Tienda",
    "slug": "ve_mi_tienda",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permitir a $(PRODUCT_NAME) acceder a tus fotos.",
        "NSPhotoLibraryAddUsageDescription": "Permitir a $(PRODUCT_NAME) guardar fotos."
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.vemitienda.online",
      "versionCode": 3
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir a $(PRODUCT_NAME) acceder a tus fotos.",
          "savePhotosPermission": "Permitir a $(PRODUCT_NAME) guardar fotos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7439d9f0-9c57-4f5f-bd72-b44bbb243a7a"
      }
    }
  }
}
